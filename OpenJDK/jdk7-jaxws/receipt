# SliTaz package receipt.

PACKAGE="jdk7-jaxws"
VERSION="2.5.1"
CATEGORY="meta"
SHORT_DESC="A component of OpenJDK 7 we use to cook icedtea7 (empty package)"
MAINTAINER="yuripourre@gmail.com"
LICENSE="GPL2"
SOURCE="jaxws"
WEB_SITE="http://java.sun.com"
TARBALL="$PACKAGE.tar.bz2"
WGET_URL="http://icedtea.wildebeest.org/download/drops/icedtea7/$VERSION/$TARBALL"
COOK_OPT="!unpack !repack_src !fs"

BUILD_DEPENDS="wget"

# Rules to gen a SliTaz package suitable for Tazpkg.
compile_rules()
{
	mkdir -p $fs/usr
}
